<p id="notice"><%= notice %></p>

<h1>School Users</h1>

<table>
  <thead>
    <tr>
      <th>School</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @school_users.each do |school_user| %>
      <tr>
        <td><%= school_user.school %></td>
        <td><%= school_user.user %></td>
        <td><%= link_to 'Show', school_user %></td>
        <td><%= link_to 'Edit', edit_school_user_path(school_user) %></td>
        <td><%= link_to 'Destroy', school_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New School User', new_school_user_path %>
